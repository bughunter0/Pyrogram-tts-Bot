{
    "name": "PyttsBot",
    "description": "Pyrogram Based Google Text To Speech Bot",
    "logo": "https://telegra.ph/file/25c3e3246dee8df0cc91c.jpg",
    "repository": "https://github.com/bughunter0",
    "keywords": [
       
        "telegram bot",
        "pyrogram"
    ],
    "env": {
        "BOT_TOKEN": {
            "description": "Your Bot token from @Botfather"
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps"
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
        },
        "OWNER_ID":{
      "description": "Owner Id",
      "required": true 
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ],
    "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ]
}
