{
    "name": "PyttsBot",
    "description": "Pyrogram Based Google Text To Speech Bot",
    "logo": "https://telegra.ph/file/25c3e3246dee8df0cc91c.jpg",
    "repository": "https://github.com/bughunter0",
    "keywords": [
       
        "telegram bot",
        "pyrogram"
    ],
    "env": {
        "BOT_TOKEN": {
            "description": "Your Bot token from @Botfather"
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps"
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
        },
        "DATABASE_URL": {
      "description": "Your MongoDB URL for Database. Ask in Support Group: https://t.me/linux_repo",
      "value": "",
      "required": true,
        "SESSION_NAME": {
      "description": "Keep this default",
      "value": "MEMBER-STICKER-BOT",
      "required": true,
        "OWNER_ID":{
      "description": "Owner Id",
      "required": true
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
